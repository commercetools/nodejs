{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            },
            "modules": "commonjs"
          }
        ],
        "stage-0"
      ],
      "plugins": [
        "array-includes",
        "transform-export-extensions",
        "transform-flow-strip-types",
        "transform-object-entries",
        "transform-object-rest-spread"
      ]
    },
    "rollup": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions"],
              "node": "current"
            },
            "modules": false
          }
        ],
        "stage-0"
      ],
      "plugins": [
        "external-helpers",
        "array-includes",
        "transform-export-extensions",
        "transform-flow-strip-types",
        "transform-object-entries",
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions"],
              "node": "6"
            },
            "modules": false
          }
        ],
        "stage-0"
      ],
      "plugins": [
        "external-helpers",
        "array-includes",
        "transform-export-extensions",
        "transform-flow-strip-types",
        "transform-object-entries",
        "transform-object-rest-spread"
      ]
    },
    "cli": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "6"
            },
            "modules": "commonjs"
          }
        ],
        "stage-0"
      ],
      "plugins": [
        "array-includes",
        "transform-export-extensions",
        "transform-flow-strip-types",
        "transform-object-entries",
        "transform-object-rest-spread"
      ]
    }
  }
}
