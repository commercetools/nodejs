{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/personal-data-erasure",
  "version": "4.0.0",
  "description": "Export and delete all data related to a single customer",
  "main": "data/personal-data-erasure.cjs.js",
  "module": "data/personal-data-erasure.es.js",
  "browser": "data/personal-data-erasure.umd.js",
  "bin": {
    "personal-data-erasure": "bin/personal-data-erasure.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "babel src/cli.js --out-dir dist --config-file '../../babel.config.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/personal-data-erasure"
  },
  "homepage": "https://commercetools.github.io/nodejs/cli/personal-data-erasure.html",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "keywords": [
    "commercetools",
    "customer",
    "get",
    "delete",
    "json",
    "personal-data"
  ],
  "files": [
    "bin",
    "dist",
    "readme.md",
    "package.json"
  ],
  "author": {
    "name": "Daniel Eriksson",
    "email": "daniel.eriksson@commercetools.com"
  },
  "license": "MIT",
  "dependencies": {
    "@commercetools/api-request-builder": "^6.0.0",
    "@commercetools/get-credentials": "^4.0.0",
    "@commercetools/sdk-client": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^7.0.0",
    "@commercetools/sdk-middleware-http": "^7.0.0",
    "@commercetools/sdk-middleware-user-agent": "^3.0.0",
    "lodash.flatten": "^4.4.0",
    "node-fetch": "^2.6.7",
    "pino": "^6.0.0",
    "pretty-error": "^2.1.1",
    "prompt-confirm": "^2.0.4",
    "yargs": "^16.0.0"
  }
}
