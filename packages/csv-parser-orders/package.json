{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/csv-parser-orders",
  "version": "3.0.0",
  "description": "Converts commercetools order data from CSV to JSON.",
  "keywords": [
    "commercetools",
    "csv",
    "nodejs",
    "parser",
    "orders",
    "json",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/csv-parser-orders"
  },
  "homepage": "https://github.com/commercetools/nodejs/tree/master/packages/csv-parser-orders#readme",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "license": "MIT",
  "contributors": [
    "Abimbola Idowu <abimbola.idowu@commercetools.com>",
    "Selwyn Versteeg <selwyn.versteeg@commercetools.com> (https://selwyn.cc/)",
    "Jan Juna <jan.juna@commercetools.com> (https://janjuna.cz/)"
  ],
  "main": "dist/csv-parser-orders.cjs.js",
  "module": "dist/csv-parser-orders.es.js",
  "browser": "dist/csv-parser-orders.umd.js",
  "bin": {
    "csvparserorder": "bin/csvparserorder.js"
  },
  "files": [
    "bin",
    "dist",
    "readme.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "cross-env NODE_ENV=production rollup -c ../../rollup.cli.config.js -n CommercetoolsSdkClient  -i ./src/cli.js -o dist/cli.js"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "csv-parser": "^3.0.0",
    "highland": "^2.13.0",
    "lodash": "^4.17.21",
    "npmlog": "^4.1.2",
    "object-path": "^0.11.4",
    "pretty-error": "^2.1.1",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "sinon": "9.2.4",
    "streamtest": "2.0.0",
    "tmp": "0.2.1"
  }
}
