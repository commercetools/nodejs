{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/customer-groups-exporter",
  "version": "4.0.0",
  "description": "Exports customer groups from the commercetools platform.",
  "main": "dist/customer-groups-exporter.cjs.js",
  "module": "dist/customer-groups-exporter.es.js",
  "browser": "dist/customer-groups-exporter.umd.js",
  "bin": {
    "customer-groups-exporter": "bin/customer-groups-exporter.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "cross-env NODE_ENV=production rollup -c ../../rollup.cli.config.js -n CommercetoolsSdkClient  -i ./src/cli.js -o dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/customer-groups-exporter"
  },
  "homepage": "https://commercetools.github.io/nodejs/cli/customer-groups-exporter.html",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "keywords": [
    "commercetools",
    "group",
    "groups",
    "json",
    "export",
    "customer",
    "cli"
  ],
  "files": [
    "bin",
    "dist",
    "readme.md",
    "package.json"
  ],
  "author": {
    "name": "Daniel Eriksson",
    "email": "daniel.eriksson@commercetools.com",
    "url": "https://github.com/daern91"
  },
  "license": "MIT",
  "dependencies": {
    "@commercetools/api-request-builder": "^6.0.0",
    "@commercetools/get-credentials": "^4.0.0",
    "@commercetools/sdk-client": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^7.0.0",
    "@commercetools/sdk-middleware-http": "^7.0.0",
    "@commercetools/sdk-middleware-user-agent": "^3.0.0",
    "JSONStream": "^1.3.5",
    "node-fetch": "^2.6.7",
    "pino": "^6.0.0",
    "pretty-error": "^2.1.1",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "streamtest": "2.0.0"
  }
}
