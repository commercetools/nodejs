{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/csv-parser-state",
  "version": "4.0.0",
  "description": "Package to parse states from CSV to JSON",
  "main": "dist/csv-parser-state.cjs.js",
  "module": "dist/csv-parser-state.es.js",
  "browser": "dist/csv-parser-state.umd.js",
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "cross-env NODE_ENV=production rollup -c ../../rollup.cli.config.js -n CommercetoolsSdkClient  -i ./src/cli.js -o dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/csv-parser-state"
  },
  "homepage": "https://commercetools.github.io/nodejs/cli/csv-parser-state.html",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "keywords": [
    "commercetools",
    "state",
    "states",
    "csv",
    "parser",
    "cli"
  ],
  "author": {
    "name": "Williams Omayuku",
    "email": "williams.omayuku@commercetools.com"
  },
  "license": "MIT",
  "bin": {
    "csv-parser-state": "bin/csv-parser-state.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "rollup-plugin-node-polyfills": "^0.2.1",
    "streamtest": "2.0.0"
  },
  "dependencies": {
    "@commercetools/api-request-builder": "^6.0.0",
    "@commercetools/get-credentials": "^4.0.0",
    "@commercetools/sdk-client": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^7.0.0",
    "@commercetools/sdk-middleware-http": "^7.0.0",
    "@commercetools/sdk-middleware-user-agent": "^3.0.0",
    "JSONStream": "^1.3.5",
    "csv-parser": "^3.0.0",
    "flat": "^5.0.0",
    "highland": "^2.13.0",
    "lodash.memoize": "^4.1.2",
    "node-fetch": "^2.6.7",
    "pino": "^6.0.0",
    "pretty-error": "^2.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "yargs": "^16.0.0"
  }
}
