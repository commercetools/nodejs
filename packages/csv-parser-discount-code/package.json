{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/csv-parser-discount-code",
  "version": "3.0.0",
  "description": "Converts commercetools discount code data from CSV to JSON.",
  "keywords": [
    "commercetools",
    "discount",
    "code",
    "csv",
    "json",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/csv-parser-discount-code"
  },
  "homepage": "https://commercetools.github.io/nodejs/cli/csv-parser-discount-code.html",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "license": "MIT",
  "contributors": [
    "Williams Omayuku <williams.omayuku@commercetools.com>"
  ],
  "main": "dist/csv-parser-discount-code.cjs.js",
  "module": "dist/csv-parser-discount-code.es.js",
  "browser": "dist/csv-parser-discount-code.umd.js",
  "bin": {
    "csvparserdiscountcode": "bin/csvparserdiscountcode.js"
  },
  "files": [
    "bin",
    "dist",
    "readme.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "babel src/cli.js --out-dir dist --config-file '../../babel.config.js'"
  },
  "dependencies": {
