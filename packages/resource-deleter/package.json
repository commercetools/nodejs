{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/resource-deleter",
  "version": "3.0.0",
  "description": "Delete resources from the commercetools platform",
  "main": "dist/resource-deleter.cjs.js",
  "module": "dist/resource-deleter.es.js",
  "browser": "dist/resource-deleter.umd.js",
  "bin": {
    "resource-deleter": "bin/resource-deleter.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "babel src/cli.js --out-dir dist --config-file '../../babel.config.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/resource-deleter"
  },
  "homepage": "https://commercetools.github.io/nodejs/cli/resource-deleter.html",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "keywords": [
    "commercetools",
    "resource",
    "resources",
    "delete"
  ],
  "author": {
    "name": "Babajide Williams",
    "email": "babajide.williams@commercetools.com"
  },
  "files": [
    "bin",
    "dist",
    "readme.md",
    "package.json"
  ],
  "license": "MIT",
  "dependencies": {
    "@commercetools/api-request-builder": "^6.0.0",
    "@commercetools/get-credentials": "^4.0.0",
    "@commercetools/sdk-client": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^7.0.0",
    "@commercetools/sdk-middleware-http": "^7.0.0",
    "@commercetools/sdk-middleware-queue": "^3.0.0",
    "@commercetools/sdk-middleware-user-agent": "^3.0.0",
    "node-fetch": "^2.6.7",
    "pino": "^6.0.0",
    "pretty-error": "^2.1.1",
    "prompts": "^2.0.4",
    "yargs": "^16.0.0",
    "core-js": "^3.22.5",
    "regenerator-runtime": "^0.13.9"
  }
}
