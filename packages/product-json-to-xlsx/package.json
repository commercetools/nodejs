{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "@commercetools/product-json-to-xlsx",
  "version": "5.1.0",
  "description": "Converts commercetools products from JSON to XLSX",
  "keywords": [
    "commercetools",
    "products",
    "projections",
    "parser",
    "export",
    "csv",
    "xlsx",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/nodejs/tree/master/packages/product-json-to-xlsx"
  },
  "homepage": "https://commercetools.github.io/nodejs/cli/product-json-to-xlsx.html",
  "bugs": "https://github.com/commercetools/nodejs/issues",
  "license": "MIT",
  "author": {
    "name": "Jan Juna",
    "email": "jan.juna-ext@commercetools.com"
  },
  "main": "dist/product-json-to-xlsx.cjs.js",
  "module": "dist/product-json-to-xlsx.es.js",
  "browser": "dist/product-json-to-xlsx.umd.js",
  "bin": {
    "product-json-to-xlsx": "bin/product-json-to-xlsx.js"
  },
  "files": [
    "bin",
    "dist",
    "readme.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -n CommercetoolsSdkClient -i ./src/index.js",
    "postbuild": "cross-env NODE_ENV=production rollup -c ../../rollup.cli.config.js -n CommercetoolsSdkClient  -i ./src/cli.js -o dist/cli.js"
  },
  "dependencies": {
    "@commercetools/get-credentials": "^4.0.0",
    "@commercetools/product-json-to-csv": "^6.0.0",
    "common-tags": "^1.8.0",
    "exceljs": "^4.0.0",
    "highland": "^2.13.0",
    "pino": "^6.0.0",
    "pretty-error": "^2.1.1",
    "slugify": "^1.3.4",
    "tmp": "^0.2.0",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "streamtest": "2.0.0",
    "unzipper": "0.10.11"
  }
}
